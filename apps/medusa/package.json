{
  "name": "medusa-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Sebastian Rindom <skrindom@gmail.com>",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "dev": "pnpm with-env pnpm build && medusa start",
    "start": "pnpm with-env pnpm build && medusa start",
    "start:custom": "pnpm with-env pnpm build && node --preserve-symlinks --trace-warnings index.js",
    "clean": "pnpm clean:dist && ./node_modules/.bin/rimraf .cache && rm -rf .turbo && rm -rf node_modules",
    "clean:dist": "pnpm with-env ./node_modules/.bin/rimraf dist",
    "build": "pnpm with-env pnpm clean:dist && pnpm build:server && pnpm build:admin",
    "build:server": "pnpm with-env pnpm clean:dist && tsc -p tsconfig.json",
    "build:admin": "pnpm with-env medusa-admin build",
    "watch": "pnpm with-env tsc --watch",
    "test": "pnpm with-env jest",
    "seed": "pnpm with-env medusa seed -f ./data/seed.json",
    "migrate": "medusa migrations run",
    "with-env": "dotenv -e .env --"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.8",
    "@medusajs/cache-inmemory": "^1.8.10",
    "@medusajs/cache-redis": "^1.9.0",
    "@medusajs/event-bus-local": "^1.9.7",
    "@medusajs/event-bus-redis": "^1.8.10",
    "@medusajs/file-local": "^1.0.2",
    "@medusajs/icons": "^1.1.0",
    "@medusajs/medusa": "^1.18.1",
    "@medusajs/ui": "^2.2.3",
    "@tanstack/react-query": "4.36.1",
    "babel-preset-medusa-package": "^1.1.19",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "16.3.1",
    "express": "^4.18.2",
    "medusa-fulfillment-manual": "^1.1.38",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-manual": "^1.0.24",
    "medusa-payment-stripe": "^6.0.6",
    "prism-react-renderer": "^2.3.0",
    "swc-loader": "^0.2.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@medusajs/medusa-cli": "^1.3.21",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.1",
    "babel-preset-medusa-package": "^1.1.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "mongoose": "^7.6.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
